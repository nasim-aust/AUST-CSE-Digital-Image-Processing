
y1 = zeros(1, 5);
y2 = zeros(1, 5);
y3 = zeros(1, 5);
y4 = zeros(1, 5);

i=1;
X1=[-360, -180, 0, 180, 360];
for x = -360 : 180 :360
  
    disp(x)
    y1(1,i) = sind(x);
    
    y2(i) = sind (2*x);
    y3(i) = sind(x)+sind(2*x);
    y4(i) = sind(x)* sind(2*x); 
end
disp(y1)
disp(y2)
plot(X1,y1,'.')
hold on;
plot(X1,y2,'*')

plot(X1,y3,'.')

plot(X1,y4,'.')

